{
	"_version": "1.5.0",
	"sap.app": {
		"id": "sapui5.app102",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"NorthwindDS": {
				"uri": "/Northwind/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sapui5.app102.view.zjsv_102_container",
			"type": "JS"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sapui5.app102.i18n.i18n"
				}
			},
			"NWList":	{
				"type": "sap.ui.model.json.JSONModel",
				"uri": "sapui5.app102.model.zjson_102_model_01"
			},
			"Northwind": {
				"dataSource": "NorthwindDS"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"viewType": "JS",
				"viewPath": "sapui5.app102.view",
				"targetControl": "appCont",
				"routerClass": "sap.m.routing.Router"
			},
			"routes": [{
				"pattern": "",
				"name": "choice",
				"view": "zjsv_102_view_01",
				"targetAggregation": "pages"
			},
			{
				"pattern": "northwind",
				"name": "northwind",
				"view": "zjsv_102_split_container",
				"targetControl": "appCont",
				"targetAggregation": "pages",
			
				"subroutes": [{
					"pattern": "northwind/",
					"name": "northwind",
					"view": "zjsv_102_master_01",
					"targetControl": "splitAppCont",
					"targetAggregation": "masterPages",
					"subroutes":[{
						"pattern": "northwind/",
						"name": "northwind",
						"view": "zjsv_102_detail_0101",
						"targetControl": "splitAppCont",
						"targetAggregation": "detailPages"
					},
					{
					"pattern": "second",
					"name": "secondView",
					"view": "zjsv_102_detail_0102",
					"targetAggregation": "detailPages"
					}]
				}]
			},
			{
            "pattern": ":all*:",
            "name": "NotFound",
            "view": "zjs_view_not_found",
            "targetAggregation": "pages"
			}]
		}
	}
}